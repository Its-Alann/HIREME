  "name": "nectar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "-": "^0.0.1",
    "@bahmutov/cypress-code-coverage": "^1.3.2",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/github-action": "^1.16.0",
